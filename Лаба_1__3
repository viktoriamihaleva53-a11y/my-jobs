#include <stdio.h>
#include <locale.h>
void main()
{
	setlocale(LC_ALL, "RUS");
	puts("*******************************************");
	puts("*        __    __    __     __   __       *");
	puts("*   |   |__|  |  |     |   |  |    |      *");
	puts("*   |     / . |__|    /_ . |__|    |      *");
	puts("*******************************************");
	return 0;
}
