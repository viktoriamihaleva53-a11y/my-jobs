#include <stdio.h>
#include <locale.h>
int main()

{
	setlocale(LC_ALL, "RUS");
	printf("123\n");  //1
	printf("1\n2\n3\n"); //2
	printf("1\n\t 2\n\t\t 3\n"); //3
	printf("%d\n\t %d\n\t\t %d\n", 1, 2, 3); //4
	printf("%10.3f\n", 12.234657); //5
	printf("%10.5f\n", 12.234657); //6
	printf("Остаток от деления %d на %d равен %d\n", 5, 2, 5 % 2); //7
	printf("Остаток от деления %d на %d равен %d\n", 7, 2, 7 % 2); //8
	printf("Произведение %d на %d равно %d\n", 2000, 4, 2000 * 4); //9
	printf("%g разделить %e равно %f\n", 5., 2000000., 5./2000000); //10
	printf("%d разделить %d равно %d\n", 5., 2000000., 5. / 2000000); //10
	printf("%f разделить %f равно %f\n", 5., 2000000., 5. / 2000000); //10
	printf("%g разделить %g равно %g\n", 5., 2000000., 5. / 2000000); //10
	printf("%e разделить %e равно %e\n", 5., 2000000., 5. / 2000000); //10

}
