#include <stdio.h>
#include <locale.h>
void name()
{
	setlocale(LC_ALL, "RUS");
	puts("*******************************************");
	puts("* тема: Разработка консольного приложения *");
	puts("*     Выполнила: Михалева В.А             *");
	puts("*******************************************");
}
void date()
{
	setlocale(LC_ALL, "RUS");
	puts("*******************************************");
	puts("*        __    __    __     __   __       *");
	puts("*   |   |__|  |  |     |   |  |    |      *");
	puts("*   |     / . |__|    /_ . |__|    |      *");
	puts("*******************************************");
}
void main()
{
	name();
	date();
}
