#include <stdio.h>
#include <locale.h>
void main()
{
	setlocale(LC_ALL, "RUS");
	puts("                 /|                   ");
	puts("                / |                   ");
	puts("               /  |                   ");
	puts("              /___|                   ");
	puts("           _______|______             ");
	puts("           \             /            ");
	puts("            \___________/             ");
	puts("                                      ");
	return 0;
}
